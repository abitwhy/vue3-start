{
  "name": "vue3-start",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open --port 8000",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "precommit": "eslint --fix",
    "test": "jest --runInBand",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run precommit",
      "git add"
    ],
    ".vscode/*": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "stylus": "^0.55.0",
    "vite": "^2.7.9"
  }
}
